".container" : {
	width:"100%",
	backgroundColor:'white',
	navBarHidden:true
},
"#webview" : {
	willHandleTouches:false,
	bottom: 60
}
"#nav" : {
	bottom: 0,
	height:60,
	width:"100%",
	opacity:0.95,
	backgroundColor:"#2D2D2D"
}
"#back" : {
	title:"<",
	left:0,
	backgroundColor: "transparent",
	font:{fontSize:24,fontWeight:"bold"}
}
"#forward" : {
	title:">",
	left:60,
	backgroundColor: "transparent",
	font:{fontSize:24,fontWeight:"bold"}
}
"#reload" : {
	title:L('reload'),
	right:10,
	width:80,
	backgroundColor: "transparent",
}
"#close" : {
	title:L('done'),
	width:80,
	backgroundColor: "transparent",
}
"#print" : {
	title:L('print'),
	right:10,
	width:Ti.UI.SIZE,
	backgroundColor: "transparent",
},
".navBtn" : {
	height:60,
	width:60,
	color: "#ffffff",
	backgroundColor:"#2D2D2D"
}
